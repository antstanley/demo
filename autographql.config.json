{
    "name": "demo",
    "schema": "./src/backend/graphql/schema/schema.gql",
    "resolvers": "./src/backend/graphql/resolvers",
    "external": "data",
    "rollup": {
        "resolve": {
            "module": false
        },
        "warning": [
            "CIRCULAR_DEPENDENCY",
            "UNRESOLVED_IMPORT"
        ]
    },
    "functions": [
        {
            "provider": "netlify",
            "dist": "./dist/graphql"
        }
    ],
    "dev": {
        "port": 7000
    }
}